<h1>UseParts#index</h1>
<p>Find me in app/views/use_parts/index.html.erb</p>


<br>

<table class='table'>
  <thead>
    <tr>
      <th>ID</th>
      <th>Qty</th>
      
      <th colspan="3"></th>

    </tr>
  </thead>

  <tbody>
    <% @use_parts.each do |use_part| %>
      <tr>
        <td><%= use_part.id %></td>
        <td><%= use_part.qty %></td>
        
        <td><%= link_to 'Show', use_part %></td>
        <td><%= link_to 'Edit', edit_product_path(use_part) %></td>
        <td><%= link_to 'Destroy', use_part, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<%= link_to 'New use part', new_use_part_path %>